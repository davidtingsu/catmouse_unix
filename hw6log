  [33mecho[m[31m [m[33m"[m[35m$1[m[33m"[m[31m [m[33m|[m bc [35m-l[m                                                                     
 [36m}[m                                                                                      
                                                                                        
[34m# sine <expression>[m                                                                     
[34m# This function prints the sine of the given expression[m                                 
[36mfunction[m [36msine {[m                                                                         
    [33mecho[m[31m [m[33m"[m[31ms ([m[35m$1[m[31m)[m[33m"[m[31m [m[33m|[m bc [35m-l[m                                                               
