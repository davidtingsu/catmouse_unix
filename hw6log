[36mfunction[m [36mangle_between {[m[18;8H [33mlocal[m [36mA[m=[35m$1[m[19;8H [33mlocal[m [36mB[m=[35m$2[m[20;8H [33mlocal[m [36mC[m=[35m$3[m[22;8H [34m# ADD CODE HERE FOR PART 1[m[24;8H [34m#An angle B is between angles A and C iff both of the following criteria are met.[m[25;8H [34m#1) cos (B-A) > cos (C-A)[m[26;8H [34m#2) cos (C-B) > cos (C-A)[m[27;8H [33mlocal[m [36mcmp1[m= float_gt [35m$( cosine $[m[33m([m[35msubtract B A[m[33m)[m[35m )[m  [35m$( cosine $[m[33m([m[35msubtract C A[m[33m)[m[35m )[m[29;8H [33mlocal[m [36mcmp2[m= float_gt [35m$( cosine $[m[33m([m[35msubtract B A[m[33m)[m[35m )[m  [35m$( cosine $[m[33m([m[35msubtract C A[m[33m)[m[35m )[m[32;8H [35m$( cosine $[m[33m([m[35msubtract B A[m[33m)[m[35m )[m  [35m$(co

}

