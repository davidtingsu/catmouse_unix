  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m-[m[33m'[m[31m [m[35m$2[m[31m [m[33m|[m bc [35m-l[m[64C[7m|[m
[36m}[m[87C[7m|[m
[36mfunction[m [36mmultiply {[m[69C[7m|[m
  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m*[m[33m'[m[31m [m[35m$2[m[31m [m[33m|[m bc [35m-l[m[64C[7m|[m
[36m}[m[87C[7m|
bash-calc.sh                                                          64,12          Bot [m[2;97H [33mlocal[m [36mcmp2[m= float_gt [35m$( cosine $[m[33m([m[35msubtract C B[m[33m)[m[35m )[m  [35m$( cosine $[m[33m([m[35msubtract C A[m[33m)[m[35m )[m[4;97H [33mif ["[m[35m$cmp1[m[33m"[m [33m-a[m [33m"[m[35m$cmp2[m[33m"][m[5;97H [33mthen[m[6;105H [34m#success[m[7;105H [33mexit[m [31m0[m[8;97H [33melse[m[9;105H [34m#failure[m[10;105H [33mecho[m[31m 1 [m[11;97H [33mfi[m[14;90H[36m}[m[16;90H[34m### Simulation Functions ###[17;90H# Variables for the state[m[18;90H[36mRUNNING[m=[31m0[m[19;90H[36mGIVEUP[m=[31m1[m[20;90H[36mCAUGHT[m=[31m2[m[22;90H[34m# does_cat_see_mouse <cat angle> <cat radius> <mouse angle>[23;90H#[24;90H# Returns true (exit code 0) if the cat can see the mouse, false otherwise.[25;90H#[26;90H# The cat sees the mouse if[27;90H# (cat radius) * cos (cat angle - mouse angle)[28;90H# is at least 1.0.[m[29;90H[36mfunction[m [36mdoes_cat_see_mouse {[m[30;97H [33mlocal[m [36mcat_angle[m=[35m$1[m[31;97H [33mlocal[m [36mcat_radius[m=[35m$2[m[32;97H [33mlocal[m [36mmouse_angle[m=[35m$3[m[34;97H [34m# ADD CODE HERE FOR PART 1[m[35;97H [33mif [[mfloat_gte [35m$(multiply $cat_radius $mouse_angle)[m 1.0[33m][m[36;97H [33mthen[m[37;105H [33mexit[m [31m0[m[38;97H [33melse[m[39;105H [33mexit[m [31m1[m[40;97H [33mfi[m[41;90H[36m}[m[43;90H[34m# next_step <current state> <current step #> <cat angle> <cat radius> <mouse angle> <max[44;90H steps>[45;90H# returns string output similar to the input, but for the next step:[46;90H# <state at next step> <next step #> <cat angle> <cat radius> <mouse angle> <max steps>[47;90H#[48;90H# exit code of this function (return value) should be the state at the next step.  This [49;90Hallows for easy[50;90H# integration into a while loop.[m[51;90H[36mfunction[m [36mnext_step {[m