  [36mans[m=[35m$(echo [m[33m"[m[35m$1[m[31m-[m[35m$integr[m[31m*[m[35m$pipi[m[33m"[m[35m [m[33m|[m[35m bc -l)[m[48C[7m|[m
  [33mecho[m[31m [m[33m"[m[35m$ans[m[33m"[m[75C[7m|[m
[36m}[m[87C[7m|[m
[36mfunction[m [36mfloat_lte {[m[68C[7m|[m
