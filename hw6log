  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m+[m[33m'[m[31m [m[35m$2[m[31m [m[33m|[m bc [35m-l[m[64C[7m|[m
[36m}[m[87C[7m|[m
[36mfunction[m [36msubtract {[m[69C[7m|[m
  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m-[m[33m'[m[31m [m[35m$2[m[31m [m[33m|[m bc [35m-l[m[64C[7m|[m
[36m}[m[87C[7m|[m
[36mfunction[m [36mmultiply {[m[69C[7m|[m
  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m*[m[33m'[m[31m [m[35m$2[m[31m [m[33m|[m bc [35m-l[m[64C[7m|[m
[36m}[m[87C[7m|
bash-calc.sh                                                          64,12          Bot [m[2;90H[36mfunction[m [36mnext_step {[m[3;97H [33mlocal[m [36mstate[m=[35m$1[m[4;97H [33mlocal[m [35m-i[m [36mstep[m=[35m$2[m[5;97H [33mlocal[m [36mold_cat_angle[m=[35m$3[m[6;97H [33mlocal[m [36mold_cat_radius[m=[35m$4[m[7;97H [33mlocal[m [36mold_mouse_angle[m=[35m$5[m[8;97H [33mlocal[m [35m-i[m [36mmax_steps[m=[35m$6[m[10;97H [33mlocal[m [36mnew_cat_angle[m=[35m${old_cat_angle}[m[11;97H [33mlocal[m [36mnew_cat_radius[m=[35m${old_cat_radius}[m[12;97H [33mlocal[m [36mnew_mouse_angle[m=[35m${old_mouse_angle}[m[14;97H [34m# First, make sure we are still running[m[15;97H [33mif [m[35m(([m [35m${state}[m [33m!=[m [35m${RUNNING}[m [35m))[m [33m;[m [33mthen[m[16;105H [33mecho[m[31m [m[35m${state}[m[31m [m[35m${step}[m[31m [m[35m${old_cat_angle}[m[31m [m[35m${old_cat_radius}[m[31m [m[35m${old_mouse_ang[17;90Hle}[m[31m [m[35m${max_steps}[m[18;105H [33mreturn[m [35m${state}[m[19;97H [33mfi[m[21;97H [34m# ADD CODE HERE FOR PART 2[m[23;97H [34m# Move the cat first[m[24;97H [33mif [m_______ [33m;[m [33mthen[m[25;105H [34m# Move the cat in if it's not at the statue and it can see the mouse[m[26;97H [33melse[m[27;105H [34m# Move the cat around if it's at the statue or it can't see the mouse[m[28;105H [34m# Check if the cat caught the mouse[m[29;97H [33mfi[m[31;97H [34m# Now move the mouse if it wasn't caught[m[32;97H [33mif [m______ [33m;[m [33mthen[m[33;105H [34m# Move the mouse[m[35;105H [34m# Give up if we're at the last step and haven't caught the mouse[m[36;105H [33mif [m_______ [33m;[m [33mthen[m[37;105H [33mfi[m[38;97H [33mfi[m[40;97H [33mecho[m[31m [m[35m${state}[m[31m [m[35m${step}[m[31m [m[35m${new_cat_angle}[m[31m [m[35m${new_cat_radius}[m[31m [m[35m${new_mouse_angle}[m[31m 