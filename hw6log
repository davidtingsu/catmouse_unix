# Variables for the state[m
[36mRUNNING[m=[31m0[m
[36mGIVEUP[m=[31m1[m
[36mCAUGHT[m=[31m2[m

[35m# does_cat_see_mouse <cat angle> <cat radius> <mouse angle>
#
# Returns [m[33mtrue[m[35m [m[33m(exit[m[35m code [m[31m0[m[33m)[m[35m if the cat can see the mouse, [m[33mfalse[m[35m otherwise.
#
# The cat sees the mouse if
# [m[33m([m[35mcat radius[m[33m)[m[35m * cos [m[33m([m[35mcat angle - mouse angle[m[33m)[m
[35m# is at least 1.0.
function does_cat_see_mouse {
        [m[33mlocal[m[35m [m[36mcat_angle[m=[35m$1
        [m[33mlocal[m[35m [m[36mcat_radius[m=[35m$2
        [m[33mlocal[m[35m [m[36mmouse_angle[m=[35m$3

        # ADD CODE HERE FOR PART [m[31m1[m[55;160H1,1[11CTop[1;1H[?12l[?25h[?25l[55;160H[K[55;1H:[?12l[?25hv[?25l[?12l[?25hs[?25l[?12l[?25hp[?25l[?12l[?25hl[?25l[?12l[?25hk[?25l[?12l[?25h[?25l[55;6H[K[55;6H[?12l[?25hi[?25l[?12l[?25ht[?25l[?12l[?25h[?25l [?12l[?25h...[?25lbash-calc.sh[?12l[?25h...[?25l[55;9Hcatmouse-fw.sh[55;23H[K[55;23H[?12l[?25h[?25l[55;22H[K[55;22H[?12l[?25h...[?25lh[55;23H[K[55;23H[?12l[?25h[?25l[55;22H[K[55;22H[?12l[?25h[?25l[55;21H[K[55;21H[?12l[?25h[?25l[55;20H[K[55;20H[?12l[?25h[?25l[55;19H[K[55;19H[?12l[?25h[?25l[55;18H[K[55;18H[?12l[?25h[?25l[55;17H[K[55;17H[?12l[?25h[?25l[55;16H[K[55;16H[?12l[?25h[?25l[55;15H[K[55;15H[?12l[?25h[?25l[55;14H[K[55;14H[?12l[?25h[?25l[55;13H[K[55;13H[?12l[?25h[?25l[55;12H[K[55;12H[?12l[?25h[?25l[55;11H[K[55;11H[?12l[?25h[?25l[55;10H[K[55;10H[?12l[?25h[?25l[55;9H[K[55;9H[?12l[?25h...[?25lbash-calc.sh[?12l[?25h[?25l"bash-calc.sh" [55;16H[K[55;16H57L, 1078C[1;89H[7m|[m
