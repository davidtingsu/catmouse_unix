[35m[46m{[m[24C [34m#+ exit from script[m[8;26H [34m#+ with appropriate error message.[m
  [36mE_PARAM_ERR[m=[31m98[m
  [36mE_ASSERT_FAILED[m=[31m99[m[13;2H [33mif [[m [33m-z[m [33m"[m[35m$2[m[33m"[m [33m][m[9C [34m#  Not enough parameters passed[m
  [33mthen[m[19C [34m#+ to assert() function.[m
    [33mreturn[m [35m$E_PARAM_ERR[m   [34m#  No damage done.[m
  [33mfi[m

  [36mlineno[m=[35m$2[m

  [33mif [[m [33m![m [35m$1[m [33m][m
