  [36mintegr[m=[35m$(echo [m[33m"[m[31mscale = 0;[m[35m$1[m[31m/[m[35m$pipi[m[33m"[m[35m [m[33m|[m[35m bc -l)[m[43C[7m|[m
  [36mans[m=[35m$(echo [m[33m"[m[35m$1[m[31m-[m[35m$integr[m[31m*[m[35m$pipi[m[33m"[m[35m [m[33m|[m[35m bc -l)[m[48C[7m|[m
  [33mecho[m[31m [m[33m"[m[35m$ans[m[33m"[m                             [46C[7m|[m
[36m}[m[87C[7m|[m
