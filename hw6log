  [36mpipi[m=[35m$(echo [m[33m"[m[31m8*a(1)[m[33m"[m[35m [m[33m|[m[35m bc -l)[m                                                         
  [36mintegr[m=[35m$(echo [m[33m"[m[31mscale = 0;[m[35m$1[m[31m/[m[35m$pipi[m[33m"[m[35m [m[33m|[m[35m bc -l)[m                                           
  [36mans[m=[35m$(echo [m[33m"[m[35m$1[m[31m-[m[35m$integr[m[31m*[m[35m$pipi[m[33m"[m[35m [m[33m|[m[35m bc -l)[m                                                
  [33mecho[m[31m [m[33m"[m[35m$ans[m[33m"[m                                                                           
[36m}[m                                                                                       
[36mfunction[m [36mfloat_lte {[m                                                                    
  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m<=[m[33m'[m[31m  [m[35m$2[m[31m [m[33m|[m bc [35m-l[m                                                              
                                                                                        
[36m}[m                                                                                       
                                                                                        
[36mfunction[m [36mfloat_lt {[m                                                                     
  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m<[m[33m'[m[31m  [m[35m$2[m[31m [m[33m|[m bc [35m-l[m                                                               
                                                                                        
[36m}[m                                                                                       
[36mfunction[m [36mfloat_eq {[m                                                                     
  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m==[m[33m'[m[31m [m[35m$2[m[31m [m[33m|[m bc [35m-l[m                                                               
[36m}[m                                                                                       
[34m#additional functions not required [m                                                     
[36mfunction[m [36mfloat_gt {[m                                                                     
  [33mecho[m[31m [m[35m$1[m[31m [m[33m'[m[31m>[m[33m'[m[31m  [m[35m$2[m[31m [m[33m|[m bc [35m-l[m                                                               
                                                                                        
[36m}[m                                                                                       
[36mfunction[m [36mfloat_gte {[m                                                                    
